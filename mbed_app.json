{  
    "config": {
        "network-interface":{
            "help": "options are ETHERNET, ETHERNET_W5500, ETHERNET_W7500x, WIFI_ESP8266, WIFI_ODIN, MESH_LOWPAN_ND, MESH_THREAD",
            "value": "ETHERNET_W5500"
        },
        "Wiznet-mosi": {
            "macro_name": "W5500_SPI_MOSI",
            "value": "PB_15"
        },
        "Wiznet-miso": {
            "macro_name": "W5500_SPI_MISO",
            "value": "PB_14"
        },
        "Wiznet-clk": {
            "macro_name": "W5500_SPI_SCLK",
            "value": "PB_13"
        },
        "Wiznet-cs": {
            "macro_name": "W5500_SPI_CS",
            "value": "PB_12"
        },
        "Wiznet-rst": {
            "macro_name": "W5500_SPI_RST",
            "value": "PB_9"
        },
        "Wiznet-spi-speed": {
            "macro_name": "MBED_CONF_W5500_SPI_SPEED",
            "value": "25000000"
        },
        "Wiznet-MAC": {
            "macro_name": "MBED_CONF_W5500_MAC",
            "value": "{0x00, 0x08, 0xdc, 0x45, 0x56, 0x67}"
        }
    },
    "target_overrides": {
        "*": {
            "mbed-http.http-buffer-size" : 2048,
            "platform.stdio-baud-rate": 115200,
            "platform.stdio-convert-newlines": true,
            "platform.all-stats-enabled": true,
            "mbed-trace.enable": 0,
            "lwip.socket-max": 8,
            "lwip.tcp-socket-max": 8         
        },
        "NUCLEO_F411RE": {
            "target.clock_source": "USE_PLL_HSI",
            "target.stdio_uart_tx": "PA_9",
            "target.stdio_uart_rx": "PA_10"
        },
        "BLACKPILL_F411CE": {
            "target.network-default-interface-type": "ETHERNET"
        },
        "BLUEPILL_F103RB": {
            "target.default_lib": "small"
        } 
    }
}